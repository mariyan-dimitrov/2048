{
  "name": "react-dev-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "store-me": "^1.5.0",
    "styled-components": "^6.0.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.8",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-styled-components": "2.1.3",
    "buffer": "6.0.3",
    "cross-env": "7.0.3",
    "eslint": "8.41.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-styled-components-a11y": "1.0.0",
    "eslint-plugin-testing-library": "5.11.0",
    "https-browserify": "1.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-canvas-mock": "2.5.1",
    "jest-runner-stylelint": "2.3.7",
    "lint-staged": "13.2.2",
    "npm-package-json-lint": "6.4.0",
    "npm-run-all": "4.1.5",
    "postcss-scss": "4.0.6",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.8",
    "react-code-blocks": "0.0.9-0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "18.2.0",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "stylelint": "14.16.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-recommended": "10.0.1",
    "stylelint-config-standard-scss": "7.0.1",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "webpack-bundle-analyzer": "4.8.0"
  },
  "scripts": {
    "dev": "cross-env GENERATE_SOURCEMAP=false REACT_APP_LOCAL_ENV=dev FAST_REFRESH=false DISABLE_ESLINT_PLUGIN=true PORT=4200 react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test --onlyChanged --maxWorkers=4 --watchAll=false --transformIgnorePatterns \"node_modules/(?!mathjs)/\"",
    "stylelint": "jest --onlyChanged --maxWorkers=4 --watchAll=false --config=jest.config.js",
    "es-lint": "eslint 'src/**/*.js' --max-warnings=0",
    "test-pre-commit": "npm-run-all --parallel --continue-on-error es-lint test stylelint",
    "analyze-bundle": "cross-env INCLUDE_BUNDLE_ANALYZER=true yarn start",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
